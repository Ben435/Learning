{
  "name": "api-gateway-wss",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "package": "npm-run-all --parallel package:*",
    "package:connect": "(export PROJECT=connect; npm run lambda-package)",
    "package:disconnect": "(export PROJECT=disconnect; npm run lambda-package)",
    "package:default": "(export PROJECT=default; npm run lambda-package)",
    "lambda-package": "cd dist/${PROJECT} && zip -r ../../out/${PROJECT}.zip * && cd ../..",
    "stack:package": "aws cloudformation package --template-file template.yml --s3-bucket deploy-temp --output-template-file package.yml",
    "stack:deploy": "aws cloudformation deploy --template-file package.yml --stack-name api-gateway-wss",
    "stack:delete": "aws cloudformation delete-stack --stack-name api-gateway-wss",
    "deploy": "npm-run-all build package stack:package stack:deploy"
  },
  "author": "Ben Clare",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "wscat": "^4.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.693.0"
  }
}
